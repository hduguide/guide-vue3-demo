
ul[data-lake-indent='1'], ol[data-lake-indent='1'] {
  margin-left: 2rem;
}

ul[data-lake-indent='2'], ol[data-lake-indent='2'] {
  margin-left: 4rem;
}

ul[data-lake-indent='3'], ol[data-lake-indent='3'] {
  margin-left: 6rem;
}

ul[data-lake-indent='4'], ol[data-lake-indent='4'] {
  margin-left: 8rem;
}

ul[data-lake-indent='5'], ol[data-lake-indent='5'] {
  margin-left: 10rem;
}

ul[data-lake-indent='6'], ol[data-lake-indent='6'] {
  margin-left: 12rem;
}

ul[data-lake-indent='7'], ol[data-lake-indent='7'] {
  margin-left: 14rem;
}

ul[data-lake-indent='8'], ol[data-lake-indent='8'] {
  margin-left: 16rem;
}

blockquote {
  display: block;
  padding-left: 10px;
  margin: 10px 0;
  border-left: 3px solid #eee;
  opacity: 0.7;
}

.lake-alert {
  display: block;
  margin: 4px 0;
  padding: 10px;
  border-radius: 4px;
  color: #262626;
  opacity: 1;
  border: 1px solid #abd2da;
  background-color: #e8f7ff;
}

.lake-alert-info {
  border: 1px solid #abd2da;
  background-color: #e8f7ff;
}

.lake-alert-warning {
  border: 1px solid #e0d1b1;
  background-color: #fffbe6;
}

.lake-alert-success {
  border: 1px solid #c2d2b5;
  background-color: #edf9e8;
}

.lake-alert-danger {
  border: 1px solid #deb8be;
  background-color: #fff3f3;
}

.lake-alert-tips {
  border: 1px solid #c3c3c3;
  background-color: #fff6b6;
}

.lake-table {
  table-layout: fixed;
  border-collapse: collapse;
  border: 1px solid #d9d9d9;
  margin: 18px 0 16px 0;
}

.lake-no-border td {
  border: none !important;
}

.lake-table tr {
  // border: 1px solid #d9d9d9;
  height: 33px;
}

.lake-table td {
  border: 1px solid #d9d9d9;
  vertical-align: top;
  padding: 4px 8px;
}

.viewer-backdrop {
  background-color: transparent !important;
}

.viewer-backdrop[class~='viewer-in'] {
  background-color: rgba(0, 0, 0, 0.5) !important;
}


.el-drawer__body {
  height: 100%;
}
