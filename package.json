{
  "name": "hduer-guide-vue3",
  "version": "0.0.2",
  "private": false,
  "description": "a knowledge base for hduers base on yuque",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build-tsc": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "format": "prettier --write ./src",
    "lint": "eslint ./src --ext .vue,.js,.ts",
    "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts",
    "prepare": "husky install",
    "test": "jest"
  },
  "repository": {
    "type": "https",
    "url": "https://github.com/hduguide/guide-vue3-demo"
  },
  "keywords": [
    "hdu",
    "hduer",
    "guide",
    "hduguide"
  ],
  "author": "hduer.cn",
  "license": "MIT",
  "bugs": {
    "url": "https://www.yuque.com/hduer/topics"
  },
  "homepage": "https://yuque.com/hduer/guide",
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "clipboard": "^2.0.8",
    "element-plus": "^1.0.2-beta.36",
    "posthtml-parser": "^0.10.0",
    "romans": "^1.1.2",
    "v-viewer": "^3.0.9",
    "vue": "^3.0.5",
    "vue-router": "^4.0.6",
    "vue3-json-viewer": "^1.0.4",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "@vitejs/plugin-vue": "^1.2.1",
    "@vitejs/plugin-vue-jsx": "^1.1.7",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/test-utils": "^2.0.0-rc.4",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
