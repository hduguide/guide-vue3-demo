    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.8.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "stylus": "^0.54.8",
    "ts-jest": "^26.5.4",
    "typescript": "^4.1.3",
    "typescript-plugin-css-modules": "^3.4.0",
    "vite": "^2.1.5",
    "vite-plugin-style-import": "^0.9.2",
    "vue-jest": "^5.0.0-alpha.7",
    "vue-tsc": "^0.0.15"
